<div id="block-{# PHP: print $block->module .'-'. $block->delta; #}" class="{{ classes }} unstyled-block"{{ attributes }}>
{{ title_prefix }}
{% if block  %}
  <h2 {{ title_attributes }}>{{ block }}</h2>
{% endif %}
{{ title_suffix }}
  <div {{ content_attributes }}>{{ content }}</div>
</div>
